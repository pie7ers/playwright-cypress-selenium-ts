{
  "name": "playwright-cypress-selenium-ts",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:selenium": "mocha -r ts-node/register selenium/html.test.ts --reporter mochawesome --reporter-options reportDir=./reports/selenium,reportFilename=selenium-test",
    "test:playwright": "playwright test playwright/html.test.ts",
    "cy:open": "cypress open",
    "cy:run": "cypress run --reporter cypress-mochawesome-reporter",
    "eslint": "eslint",
    "act:eslint": "act -j eslint",
    "act:test:selenium": "act -j test-selenium -s ACT=true --container-architecture linux/amd64",
    "act:test:playwright": "act -j test-playwright -s ACT=true",
    "act:test:cy": "act -j test-cypress -s ACT=true"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@commitlint/config-conventional": "^19.8.1",
    "@cypress/webpack-preprocessor": "^7.0.1",
    "@playwright/test": "^1.55.0",
    "@types/chai": "^5.2.2",
    "@types/mocha": "^10.0.10",
    "@types/mochawesome": "^6.2.4",
    "@types/node": "^24.3.1",
    "@types/selenium-webdriver": "^4.1.28",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "chai": "^4.5.0",
    "commitlint": "^19.8.1",
    "cypress": "^15.1.0",
    "cypress-mochawesome-reporter": "^4.0.2",
    "eslint": "^9.35.0",
    "eslint-plugin-cypress": "^5.1.1",
    "jiti": "^2.5.1",
    "mocha": "^10.8.2",
    "mochawesome": "^7.1.3",
    "selenium-webdriver": "^4.39.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
